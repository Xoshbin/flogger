{
  "name": "xoshbin/flogger",
  "description": "Beautifull Logger for Filament",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Xoshbin\\Flogger\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Khoshbin",
      "email": "xoshbin@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^8.1",
    "illuminate/console": "^10.0|^11.0|^12.0",
    "illuminate/database": "^10.0|^11.0|^12.0",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "filament/filament": "^3.2|^4.0|^5.0",
    "filament/support": "^3.2|^4.0|^5.0",
    "spatie/laravel-package-tools": "^1.16.1"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Xoshbin\\Flogger\\FloggerServiceProvider"
      ]
    }
  },
  "require-dev": {
    "larastan/larastan": "^2.2",
    "orchestra/testbench": "^8.20|^9.0",
    "phpstan/phpstan": "v1.11.1",
    "laravel/pint": "^1.0",
    "pestphp/pest": "^2.0",
    "pestphp/pest-plugin-laravel": "^2.0"
  },
  "prefer-stable": true,
  "config": {
    "sort-package": true,
    "preferred-install": "dist",
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "scripts": {
    "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
    "analyse": "vendor/bin/phpstan analyse",
    "test": "vendor/bin/pest",
    "test-coverage": "vendor/bin/pest --coverage",
    "pint": "vendor/bin/pint"
  }
}